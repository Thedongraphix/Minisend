{
  "name": "minisend",
  "version": "1.0.0",
  "private": true,
  "description": "Farcaster mini app for converting USDC to mobile money via PayCrest. Seamless crypto-to-fiat transactions with research-based polling implementation.",
  "keywords": [
    "usdc",
    "paycrest",
    "farcaster",
    "miniapp",
    "crypto",
    "offramp",
    "mobile-money",
    "base-network",
    "polling",
    "settlement-detection",
    "supabase"
  ],
  "author": "Minisend Team",
  "pnpm": {
    "overrides": {
      "@base-org/account": "latest"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "bufferutil",
      "utf-8-validate",
      "keccak",
      "unrs-resolver",
      "protobufjs",
      "@reown/appkit"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next build && next start",
    "lint": "next lint",
    "setup-supabase": "node scripts/setup-supabase.js",
    "setup-auth": "node scripts/setup-auth-schema.js",
    "verify-auth": "node scripts/verify-auth-setup.js",
    "test-supabase": "node -e \"require('./lib/supabase/config').testConnection().then(console.log)\"",
    "health-check": "node -e \"require('./lib/supabase/config').checkHealth().then(console.log)\"",
    "test-db-quick": "node scripts/quick-db-test.js",
    "test-db-full": "node test-database-integration.js",
    "test-notification": "tsx scripts/test-notifications.ts",
    "inspect-fid": "tsx scripts/inspect-fid-tracking.ts",
    "backfill-farcaster": "tsx scripts/backfill-farcaster-profiles.ts"
  },
  "dependencies": {
    "@base-org/account-ui": "^1.0.1",
    "@coinbase/onchainkit": "^0.38.19",
    "@farcaster/frame-sdk": "^0.0.64",
    "@farcaster/frame-wagmi-connector": "^0.0.53",
    "@farcaster/miniapp-node": "^0.1.11",
    "@farcaster/miniapp-sdk": "^0.2.1",
    "@farcaster/miniapp-wagmi-connector": "^1.1.0",
    "@neynar/nodejs-sdk": "^3.80.0",
    "@neynar/react": "^1.2.20",
    "@pigment-css/react": "^0.0.30",
    "@privy-io/react-auth": "^3.10.1",
    "@supabase/supabase-js": "^2.52.0",
    "@tanstack/react-query": "^5",
    "@types/node-forge": "^1.3.13",
    "@types/pg": "^8.15.4",
    "@types/qrcode": "^1.5.5",
    "@upstash/redis": "^1.34.4",
    "framer-motion": "^12.23.25",
    "html-to-image": "^1.11.13",
    "jspdf": "^3.0.2",
    "next": "15.5.9",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1",
    "permissionless": "^0.2.57",
    "pg": "^8.11.3",
    "posthog-js": "^1.261.0",
    "qr-code-styling": "^1.9.2",
    "qrcode": "^1.5.4",
    "react": "^18",
    "react-dom": "^18",
    "viem": "^2.27.2",
    "wagmi": "^2.14.11"
  },
  "devDependencies": {
    "@types/node": "^20.19.10",
    "@types/pg": "^8.15.4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^17.2.3",
    "eslint": "^8",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
